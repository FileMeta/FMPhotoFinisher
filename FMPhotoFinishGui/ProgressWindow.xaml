<Window x:Class="FMPhotoFinisher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FileMeta Collect from Camera" Height="550" Width="700" Loaded="Window_Loaded">
    <DockPanel>
        <TextBox Name="m_Progress" DockPanel.Dock="Top"/>
        <FlowDocumentScrollViewer Name="m_BodyViewer" ScrollViewer.VerticalScrollBarVisibility="Auto">
            <FlowDocument Name="m_BodyDoc" FontFamily="Calibri" TextAlignment="Left"/>
        </FlowDocumentScrollViewer>
    </DockPanel>
</Window>
